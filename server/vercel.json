{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "status":200,
            "headers": {
                "Access-Control-Allow-Origin": "https://richpanel-stripe.vercel.app",
                "Access-Control-Allow-Credentials" : true
            }
        }
    ]
}
